<AnchorPane fx:id="studentViewPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.student.StudentViewController">
   <children>
      <AnchorPane prefHeight="744.0" prefWidth="930.0">
         <children>
            <Label alignment="CENTER" prefHeight="46.0" prefWidth="929.0" style="-fx-background-color: #44e795;" textFill="#383c3a">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="btnClose" fitHeight="70.0" fitWidth="44.0" layoutX="886.0" layoutY="1.0" onMouseClicked="#closeOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Close_Window_48px.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnMinimize" fitHeight="70.0" fitWidth="44.0" layoutX="842.0" layoutY="1.0" onMouseClicked="#minimizeOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Minimize_Window_48px.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="70.0" fitWidth="44.0" layoutX="5.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/student.png" />
               </image>
            </ImageView>
            <Label alignment="CENTER" layoutX="56.0" prefHeight="46.0" prefWidth="152.0" text="STUDENT" textFill="#383c3a">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
            <TextArea fx:id="studentTextArea" editable="false" layoutX="49.0" layoutY="71.0" onDragDetected="#studentTextAreaOnAction" prefHeight="419.0" prefWidth="834.0" />
            <TextArea fx:id="txtStumsg" layoutX="56.0" layoutY="549.0" onDragDetected="#stuMsgOnAction" prefHeight="88.0" prefWidth="667.0" />
            <Label layoutX="56.0" layoutY="509.0" prefHeight="24.0" prefWidth="206.0" text="Type your question here?" />
            <Button fx:id="btnSubmit" layoutX="771.0" layoutY="571.0" mnemonicParsing="false" onAction="#submitOnAction" prefHeight="36.0" prefWidth="115.0" style="-fx-background-color: #ff5005;" text="Submit" />
            <ImageView fx:id="lblLogOut" fitHeight="70.0" fitWidth="44.0" layoutX="56.0" layoutY="662.0" onMouseClicked="#logoutOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Logout_Rounded_Down_48px.png" />
               </image>
            </ImageView>
            <Label layoutX="104.0" layoutY="676.0" prefHeight="18.0" prefWidth="69.0" text="Log out">
               <font>
                  <Font name="System Bold" size="12.0" />
               </font>
            </Label>
            <ImageView fx:id="lblSettings" fitHeight="70.0" fitWidth="44.0" layoutX="798.0" layoutY="640.0" onMouseClicked="#stuSettingsOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Settings_48px.png" />
               </image>
            </ImageView>
            <Label layoutX="799.0" layoutY="685.0" prefHeight="18.0" prefWidth="58.0" text="Settings">
               <font>
                  <Font name="System Bold" size="12.0" />
