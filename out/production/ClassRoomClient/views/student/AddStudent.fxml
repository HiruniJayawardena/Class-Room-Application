<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="addStuPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.student.AddStudentController">
   <children>
      <AnchorPane prefHeight="444.0" prefWidth="846.0">
         <children>
            <Label alignment="CENTER" prefHeight="46.0" prefWidth="846.0" style="-fx-background-color: #44e795;" textFill="#383c3a">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
            <ImageView fx:id="btnClose" fitHeight="70.0" fitWidth="44.0" layoutX="802.0" layoutY="1.0" onMouseClicked="#closeOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Close_Window_48px.png" />
               </image>
            </ImageView>
            <ImageView fx:id="btnMinimize" fitHeight="70.0" fitWidth="44.0" layoutX="758.0" layoutY="1.0" onMouseClicked="#minimizeOnAction" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Minimize_Window_48px.png" />
               </image>
            </ImageView>
            <ImageView fitHeight="70.0" fitWidth="44.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../images/icons8_Add_User_Male_48px.png" />
               </image>
            </ImageView>
            <JFXTextField fx:id="txtStuId" labelFloat="true" layoutX="62.0" layoutY="96.0" onAction="#stuIdOnAction" prefHeight="26.0" prefWidth="315.0" promptText="StudentId" />
            <JFXTextField fx:id="txtStuNIC" labelFloat="true" layoutX="61.0" layoutY="211.0" onAction="#stuNICOnAction" prefHeight="26.0" prefWidth="315.0" promptText="NIC:" />
            <JFXTextField fx:id="txtStuName" labelFloat="true" layoutX="61.0" layoutY="152.0" onAction="#stuNameOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Name:" />
            <JFXTextField fx:id="txtStuTp" labelFloat="true" layoutX="464.0" layoutY="96.0" onAction="#stuTpOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Contact No:" />
            <Label alignment="CENTER" layoutX="51.0" prefHeight="46.0" prefWidth="247.0" text="ADD STUDENT" textFill="#383c3a">
               <font>
                  <Font name="Times New Roman Bold" size="24.0" />
               </font>
            </Label>
            <Button fx:id="btnAddStudent" layoutX="453.0" layoutY="315.0" mnemonicParsing="false" onAction="#addStudentOnAction" prefHeight="34.0" prefWidth="198.0" style="-fx-background-color: yellow;" text="Add" />
         </children>
      </AnchorPane>
      <ImageView fx:id="btnHome" fitHeight="70.0" fitWidth="44.0" layoutX="760.0" layoutY="360.0" onMouseClicked="#homeOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons8_Home_48px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
