<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="addLecturerPane" prefHeight="444.0" prefWidth="846.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.lecturer.AddLecturerController">
   <children>
      <Label alignment="CENTER" prefHeight="46.0" prefWidth="846.0" style="-fx-background-color: #44e795;" textFill="#383c3a">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <ImageView fx:id="btnClose" fitHeight="70.0" fitWidth="44.0" layoutX="802.0" layoutY="1.0" onMouseClicked="#closeOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons8_Close_Window_48px.png" />
         </image>
      </ImageView>
      <ImageView fx:id="btnMinimize" fitHeight="70.0" fitWidth="44.0" layoutX="758.0" layoutY="1.0" onMouseClicked="#minimizeOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons8_Minimize_Window_48px.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="70.0" fitWidth="44.0" layoutY="1.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons8_Add_User_Male_48px.png" />
         </image>
      </ImageView>
      <JFXTextField fx:id="txtLecturerId" labelFloat="true" layoutX="62.0" layoutY="96.0" onAction="#lecturerIdOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Lecturer Id:" />
      <JFXTextField fx:id="txtLessonId" labelFloat="true" layoutX="464.0" layoutY="152.0" onAction="#lessonIdOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Lesson Id:" />
      <JFXTextField fx:id="txtLecNIC" labelFloat="true" layoutX="61.0" layoutY="211.0" onAction="#lecNICOnAction" prefHeight="26.0" prefWidth="315.0" promptText="NIC:" />
      <JFXTextField fx:id="txtLecName" labelFloat="true" layoutX="61.0" layoutY="152.0" onAction="#lecNameOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Name:" />
      <JFXTextField fx:id="txtLecTp" labelFloat="true" layoutX="464.0" layoutY="96.0" onAction="#lecTpOnAction" prefHeight="26.0" prefWidth="315.0" promptText="Contact No:" />
      <Label alignment="CENTER" layoutX="51.0" prefHeight="46.0" prefWidth="247.0" text="ADD LECTURER" textFill="#383c3a">
         <font>
            <Font name="Times New Roman Bold" size="24.0" />
         </font>
      </Label>
      <Button fx:id="btnAddLecturer" layoutX="453.0" layoutY="315.0" mnemonicParsing="false" onAction="#addLecturerOnAction" prefHeight="34.0" prefWidth="198.0" style="-fx-background-color: yellow;" text="Add" />
      <ImageView fx:id="btnHome" fitHeight="70.0" fitWidth="44.0" layoutX="770.0" layoutY="370.0" onMouseClicked="#homeOnAction" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/icons8_Home_48px.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
